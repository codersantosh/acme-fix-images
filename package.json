{
	"name": "acme-fix-images",
	"version": "1.0.0",
	"description": "Fix image sizes after you have changed image sizes from Media Settings.",
	"author": "codersantosh",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build:admin": "wp-scripts build src/admin.js --output-path=build/admin",
		"build": "npm-run-all build:admin",
		"start:admin": "wp-scripts start src/admin.js --output-path=build/admin",
		"start": "npm-run-all --parallel start:admin",
		"copy": "node .bin/copy.js",
		"replace": "node .bin/replace-texts.js",
		"makejsstring": "node .bin/make-string.js",
		"makepot": "wp i18n make-pot . languages/acme-fix-images.pot --skip-js --exclude=src,vendor,node_modules --merge=languages/js.pot",
		"deploy": "npm-run-all build replace makejsstring makepot copy",
		"packages-update": "wp-scripts packages-update"
	},
	"browserslist": [
		"last 3 versions",
		"> 1%"
	],
	"dependencies": {
		"atrc": "^1.0.30",
		"classnames": "^2.5.1"
	},
	"devDependencies": {
		"@wordpress/babel-plugin-makepot": "^6.10.0",
		"@wordpress/babel-preset-default": "^8.10.0",
		"@wordpress/i18n": "^5.10.0",
		"@wordpress/scripts": "^30.3.0",
		"autoprefixer": "^10.4.17",
		"babel-cli": "^6.26.0",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"replace-in-file": "^7.1.0"
	}
}
